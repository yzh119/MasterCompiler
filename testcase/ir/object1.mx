class Frog{
	int age;
	void init() {
		this.age = 0;
	}
	void add_s(int x) {
		this.age = this.age + x;
	}
	void print() {
		println(toString(this.age));
	}
}
int MAXN = 100;
int main() {
	Frog[] professor = new Frog[MAXN];
	int i;
	for (i = 0; i < MAXN; i++) {
		professor[i] = new Frog;
		professor[i].init();
	}
	for (i = 0; i < MAXN; i++) {
		professor[i].add_s(i);
	}
	for (i = 0; i < MAXN; i++) {
		professor[i].print();
	}
	return 0;
}